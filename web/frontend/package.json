{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "test": "NEXT_PUBLIC_TESTING_MODE=true NEXT_PUBLIC_API_URL=http://localhost:8000 NEXT_PUBLIC_MOCK_AUDIO=true playwright test",
    "test:mock": "NEXT_PUBLIC_TESTING_MODE=true NEXT_PUBLIC_API_URL=http://localhost:8000 NEXT_PUBLIC_MOCK_AUDIO=true playwright test",
    "test:real": "NEXT_PUBLIC_TESTING_MODE=true NEXT_PUBLIC_API_URL=http://localhost:8000 NEXT_PUBLIC_MOCK_AUDIO=false playwright test",
    "test:mock-audio": "NEXT_PUBLIC_TESTING_MODE=true NEXT_PUBLIC_API_URL=http://localhost:8000 NEXT_PUBLIC_MOCK_AUDIO=true playwright test tests/e2e/mock-audio-test.spec.ts",
    "test:core": "NEXT_PUBLIC_TESTING_MODE=true NEXT_PUBLIC_API_URL=http://localhost:8000 NEXT_PUBLIC_MOCK_AUDIO=true playwright test tests/e2e/core-functionality.spec.ts",
    "test:all": "NEXT_PUBLIC_TESTING_MODE=true NEXT_PUBLIC_API_URL=http://localhost:8000 playwright test",
    "test:debug": "NEXT_PUBLIC_TESTING_MODE=true NEXT_PUBLIC_API_URL=http://localhost:8000 PWDEBUG=1 playwright test",
    "test:watch": "NEXT_PUBLIC_TESTING_MODE=true NEXT_PUBLIC_API_URL=http://localhost:8000 playwright test --watch",
    "cleanup-tests": "./scripts/cleanup-test-artifacts.sh"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^18.0.1",
    "@heroicons/react": "^2.2.0",
    "@types/uuid": "^10.0.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.357.0",
    "next": "14.1.3",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^2.2.1",
    "uuid": "^11.1.0",
    "videocontext": "^0.54.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.51.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.16",
    "eslint": "^9",
    "eslint-config-next": "15.2.1",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
